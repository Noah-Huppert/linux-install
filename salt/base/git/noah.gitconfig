[user]
	name = Noah Huppert
	email = {{ pillar.git.emails.noah }}
	signingkey = {{ pillar.gpg.user_keys.noah }}
[alias]
	a = add .
	s = status
	d = diff
	dc = diff --cached
	c = commit -S
	p = push
	po = push origin
	pom = push origin master
	poma = push origin main
	pu = pull
	pul = pull origin master
	pula = pull origin main
	co = checkout
	b = branch
[push]
	default = current
[gpg]
	program = gpg2
[commit]
    gpgsign = true
