            "cmd": "swaylock -f -c 000000 --image {{ pillar.rice.images_directory }}/{{ pillar.rice.lock_img }}"
          },
          {
            "name": "Sleep",
            "cmd": "systemctl suspend"
          },
          {
            "name": "Hibernate",
            "cmd": "systemctl hibernate"
          },
          {
            "name": "Logout",
            "cmd": "swaymsg exit"
          },
          {
            "name": "Reboot",
            "cmd": "systemctl reboot"
          },
          {
            "name": "Shutdown",
            "cmd": "systemctl -i poweroff"
          }
        ]
      }
